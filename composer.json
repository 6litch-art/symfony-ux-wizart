{
    "name": "xkzl/wizart-tech-api",
    "type": "symfony-bundle",
    "description": "Wizart Tech API integration for Symfony",
    "keywords": ["symfony", "wizart", "tech", "api"],
    "homepage": "https://gitlab.glitchr.io/repository-public/symfony/bundles",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyerconde@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Wizart\\Tech\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Wizart\\Tech\\": "./Tests"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^5.0|^6.0",
        "symfony/dependency-injection": "^5.0|^6.0",
        "symfony/http-kernel": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/framework-bundle": "^5.0|^6.0",
        "symfony/twig-bundle": "^5.0|^6.0",
        "symfony/var-dumper": "^5.0|^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}

